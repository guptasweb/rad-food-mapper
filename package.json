{
  "name": "rad-food-mapper",
  "version": "1.0.0",
  "private": true,
  "description": "Backend API for searching San Francisco Mobile Food Facilities",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev:server": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:client": "npm --prefix client run dev",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "test": "jest --runInBand",
    "lint": "echo 'no linter configured'",
    "cy:open": "cd client && npx cypress open --e2e",
    "cy:run": "cd client && npx cypress run --e2e --browser chrome"
  },
  "keywords": [
    "express",
    "typescript",
    "san-francisco",
    "food-trucks"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3"
  }
}

